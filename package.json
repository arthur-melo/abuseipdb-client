{
  "name": "abuseipdb-client",
  "version": "0.1.52",
  "private": false,
  "description": "AbuseIPDB Node.js API client.",
  "author": "Arthur Melo <contact@arthurmelo.com>",
  "homepage": "https://github.com/arthur-melo/abuseipdb-client",
  "license": "MIT",
  "engines": {
    "node": ">=14.18.0"
  },
  "sideEffects": false,
  "types": "./dist/types/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup --config rollup.config.js",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --coverage",
    "lint": "prettier -w '{lib,test}/**/*.ts' && eslint --fix '{lib,test}/**/*.ts'",
    "docs": "typedoc",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "fetch-blob": "^3.2.0",
    "formdata-polyfill": "^4.0.10",
    "isomorphic-unfetch": "^4.0.1",
    "jsonapi-typescript": "^0.1.3",
    "validator": "^13.7.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.2",
    "@types/jest": "^29.2.4",
    "@types/node-fetch": "^2.6.2",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^4.0.4",
    "rollup": "^3.7.5",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^20.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-replace-text": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-melo/abuseipdb-client.git"
  },
  "bugs": {
    "url": "https://github.com/arthur-melo/abuseipdb-client/issues"
  },
  "keywords": [
    "typescript",
    "abuseipdb",
    "node"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
