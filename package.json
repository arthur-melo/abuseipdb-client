{
  "name": "abuseipdb-client",
  "version": "2.0.33",
  "private": false,
  "description": "AbuseIPDB Node.js API client.",
  "author": "Arthur Melo <contact@arthurmelo.com>",
  "homepage": "https://github.com/arthur-melo/abuseipdb-client",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "types": "./dist/types/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup --config rollup.config.js",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --coverage",
    "lint": "prettier -w '{lib,test}/**/*.ts' && eslint --fix '{lib,test}/**/*.ts'",
    "docs": "typedoc --plugin typedoc-plugin-replace-text",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "husky",
    "semantic-release": "cross-env HUSKY=0 semantic-release"
  },
  "dependencies": {
    "jsonapi-typescript": "^0.1.3",
    "validator": "^13.15.20",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.1",
    "@types/jest": "^30.0.0",
    "@types/validator": "^13.15.4",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "jest-extended": "^7.0.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-replace-text": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-melo/abuseipdb-client.git"
  },
  "bugs": {
    "url": "https://github.com/arthur-melo/abuseipdb-client/issues"
  },
  "keywords": [
    "typescript",
    "abuseipdb",
    "node"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
